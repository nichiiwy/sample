a = int(input("Enter a number larger than 100: "))
b = int(input("Enter a number smaller than 10: "))

if a > 100 and b < 10:    
    numoftimes = a//b
    print("The smaller number fits into the larger number", (numoftimes), "times.")

else:
    if a <= 100:
        print ("Error, please enter a number greater than 100")
        a = int(input("Enter a number larger than 100: "))
    if b >= 10:
        print("Error, please enter a number smaller than 10")
        b = int(input("Enter a number smaller than 10: "))
    numoftimes = a//b
    print("The smaller number fits into the larger number", (numoftimes), "times.")
  
  
