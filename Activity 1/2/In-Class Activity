num = int(input("Please enter 1, 2, or 3: "))
if num == 1:
    print("Thank you")
elif num == 2:
    print("Well done")
elif num ==3:
    print("Correct")
else:
    print("Error message")



days = input("Number of days please: ")
days = int(days)
hours = days*24
minutes = hours*60
seconds = minutes*60
print("There are", (hours), "hours", (minutes), "minutes", (seconds), "seconds in", (days), "number of days")
